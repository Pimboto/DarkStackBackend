{
  "name": "bsky-bot",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start:minimal": "node dist/minimal-bot.js",
    "start:basic": "node dist/examples/basicBot.js",
    "start:chat": "node dist/examples/chatBot.js",
    "start:engagement": "node dist/examples/engagementBot.js",
    "dev:minimal": "nodemon --exec \"node --no-warnings=ExperimentalWarning --loader ts-node/esm\" src/minimal-bot.ts",
    "dev:basic": "nodemon --exec \"node --no-warnings=ExperimentalWarning --loader ts-node/esm\" src/examples/basicBot.ts",
    "dev:chat": "nodemon --exec \"node --no-warnings=ExperimentalWarning --loader ts-node/esm\" src/examples/chatBot.ts",
    "dev:engagement": "nodemon --exec \"node --no-warnings=ExperimentalWarning --loader ts-node/esm\" src/examples/engagementBot.ts",
    "debug:basic": "nodemon --exec \"node --no-warnings=ExperimentalWarning --loader ts-node/esm\" src/examples/debug-basicBot.ts",
    "lint": "eslint src --ext .ts",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@atproto/api": "^0.9.0",
    "@atproto/bsky": "^0.0.123",
    "@skyware/bot": "^0.3.8",
    "dotenv": "^16.0.3",
    "http-proxy-agent": "^7.0.0",
    "https-proxy-agent": "^7.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
